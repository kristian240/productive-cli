  const res = await fetch(`https://api.productive.io/api/v2/${path}`, {
    method: 'PUT',
    body: JSON.stringify(data),
    headers,
  });
  return await res.json();
}

async function destroy(path, headers) {
  const res = await fetch(`https://api.productive.io/api/v2/${path}`, {
    method: 'DELETE',
    headers,
  });
  return await res.json();
}

