async function findDeal(search, headers, today) {
  const matchingDeal = await get(
    `deals?filter[query]=${search}&filter[date][lt_eq]=${today}&filter[end_date][gt_eq]=${today}`,
    headers
  );
  return matchingDeal.data.map((d) => ({
